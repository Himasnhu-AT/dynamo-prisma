{
  "name": "dynamo-prisma",
  "version": "1.0.0",
  "description": "Create Prisma Models Dynamically",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "generate-schema": "tsc schemaGenerator.ts && node schemaGenerator.js",
    "verify-schema": "npx prisma validate"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/testing": "^10.3.3",
    "prisma-schema-dsl": "^2.2.0",
    "prisma-schema-dsl-types": "^1.1.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": ".",
    "testEnvironment": "node"
  }
}
